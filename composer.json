{
    "name": "richarddobron/laravel-localized-routes",
    "description": "Laravel Package for managing localized routes in Laravel applications.",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "richarddobron\\LocalizedRoutes\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
          "richarddobron\\LocalizedRoutes\\Tests\\": "tests/"
        }
    },
    "scripts": {
      "post-merge": "composer install",
      "tests": "vendor/bin/phpunit --colors=always --testdox",
      "fix-style": "vendor/bin/php-cs-fixer fix --allow-risky=yes",
      "check-style": "vendor/bin/php-cs-fixer fix --allow-risky=yes --dry-run"
    },
    "authors": [
        {
          "name": "Richard Dobro≈à"
        },
        {
            "name": "Matteo Palazzo",
            "email": "outlaw.plz@gmail.com"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
              "richarddobron\\LocalizedRoutes\\LocalizedRouteServiceProvider"
            ],
            "aliases": {
              "LocalizedRoute": "richarddobron\\LocalizedRoutes\\Facades\\LocalizedRoute"
            }
        }
    },
    "keywords": [
        "laravel",
        "routes",
        "localization"
    ],
    "require": {
      "php": "^8.0",
      "illuminate/support": "^9.0|^10.0|^11.0",
      "illuminate/routing": "^9.0|^10.0|^11.0",
      "illuminate/http": "^9.0|^10.0|^11.0",
      "ramsey/collection": "^1.3|^2.0"
    },
    "require-dev": {
      "phpunit/phpunit": "^9.5|^10.0",
      "orchestra/testbench": "^7.0|^8.0|^9.0",
      "friendsofphp/php-cs-fixer": "^3.55"
    },
    "config": {
        "audit": {
          "block-insecure": false
        }
    }
}
